{
  "name": "zos-swap-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for ZOS Swap API",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
